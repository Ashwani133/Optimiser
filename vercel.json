{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.css)",  
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.js)",   
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.html)", 
      "dest": "/$1"
    },
    {
      "src": "/signup",
      "dest": "/signup.html"
    },
    {
      "src":"/todo",
      "dest":"todo.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/about/contact",
      "dest": "/nested/contact.html"
    },
    {
      "src": "/about/team",
      "dest": "/nested/team.html"
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}
